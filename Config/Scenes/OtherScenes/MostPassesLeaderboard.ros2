<Simulation>

  <Include href="../Includes/NaoV6H25.rsi2"/>
  <Include href="../Includes/Ball2016SPL.rsi2"/>
  <Include href="../Includes/Field2020SPL.rsi2"/>
  <Include href="../Includes/MostPassesIllegalArea.rsi2"/>

  <Scene name="RoboCup" controller="SimulatedNao" stepLength="0.012" color="rgb(65%, 65%, 70%)">
    <PointLight z="9m" ambientColor="rgb(50%, 50%, 50%)"/>

    <Compound name="robots">
      <Body ref="Nao" name="robot1">
        <Translation x="3.2" y="-3" z="320mm"/>
        <Rotation z="90degree"/>
        <Set name="TeamColor" value="black"/>
      </Body>
      <Body ref="Nao" name="robot2">
        <Translation x="3.2" y="3" z="320mm"/>
        <Rotation z="-90degree"/>
        <Set name="TeamColor" value="black"/>
      </Body>
    </Compound>

    <Compound name="teams">
      <Compound name="B-Human">
        <Compound name="5"/>
        <Compound name="black"/>
        <Compound name="purple"/>
      </Compound>
      <Compound name="B-Team">
        <Compound name="70"/>
        <Compound name="red"/>
        <Compound name="blue"/>
      </Compound>
    </Compound>

    <Compound name="extras"/>

    <Compound name="balls">
      <Body ref="ball">
        <Translation x="2.85" y="2" z="0.5m"/>
      </Body>
    </Compound>

    <Compound name="scenarios">
      <Compound name="MostPassesLeaderboard"/>
    </Compound>

    <Compound ref="field"/>
    <Compound ref="illegalField"/>

  </Scene>
</Simulation>
